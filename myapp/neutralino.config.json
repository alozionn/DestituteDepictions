{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/my-app/build",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "Destitute Depictions",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/my-app/public/logo192.png",
      "borderless": true
    }
  },
  "cli": {
    "binaryName": "myapp",
    "resourcesPath": "/my-app/build",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/my-app/public/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "frontendLibrary": {
      "patchFile": "/my-app/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  },
  "maybe-enableExtensions": true,
  "maybe-extensions": [
    {
      "id": "js.neutralino.ddserver",
      "command": "node ${NL_PATH}/extensions/binary/main.js"
    }
  ]
}